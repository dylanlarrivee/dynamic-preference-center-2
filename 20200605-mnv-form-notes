var counter = 0;
let playerArray = [];
function PlayerData (name,number,position,experience,headshot) {
    this.playerName = name,
    this.playerNumber = number,
    this. playerPosition = position,
    this.playerExperience = experience,
    this.playerHeadshotURL = headshot 
}

